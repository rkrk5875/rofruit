local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))() local Window = Library.CreateLib("Ro Fruit", "DarkTheme") local Tab = Window:NewTab("auto farm") local Section = Tab:NewSection("auto farm") Section:NewDropdown("auto farm", "", {"Pirate [Lv.1]", "Buggy [Lv.20]", "Monkey [Lv.25]","Angel MLG [Lv.75]","Dubai [Lv.100]","Sandia kimistuu [Lv.200]","??? [Lv.250]","Monkey Training [Lv.300]","Thor [Lv.350]","Enma [Lv.400]","Ryuma [Lv.500]","Shank [Lv.900]","Mihawk [Lv.1000]"}, function(mode)     _G.mode = mode end) Section:NewSlider("auto farm reach", "", 20, 0, function(s)  _G.re = s end) Section:NewDropdown("auto farm mode", "", {"1", "2", "3"}, function(re) _G.mode2 = re end) Section:NewToggle("auto farm on", "", function(state)     if state then         _G.autofarm = true        if _G.mode2 == "1" then         while _G.autofarm do task.wait()             pcall(function()         for i, v in pairs(game:GetService("Workspace").Mob:GetChildren()) do             if v.Name == _G.mode then             repeat task.wait() game:GetService("ReplicatedStorage").Document.Remote.Stats:FireServer("Melee","Up",1)             game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,0,_G.re)             game:GetService("ReplicatedStorage").Document.Combat.Combat:FireServer()             until v.Humanoid.Health == 0 or not v:FindFirstChild("Humanoid") or not _G.autofarm                 end end end) end elseif _G.mode2 == "2" then      while _G.autofarm do task.wait()             pcall(function()         for i, v in pairs(game:GetService("Workspace").Mob:GetChildren()) do             if v.Name == _G.mode then             repeat task.wait() game:GetService("ReplicatedStorage").Document.Remote.Stats:FireServer("Melee","Up",1)             game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,_G.re,0) * CFrame.Angles(math.rad(270),0,0)             game:GetService("ReplicatedStorage").Document.Combat.Combat:FireServer() until v.Humanoid.Health == 0 or not v:FindFirstChild("Humanoid") or not _G.autofarm                 end end end) end elseif _G.mode2 == "3" then         while _G.autofarm do task.wait()             pcall(function()         for i, v in pairs(game:GetService("Workspace").Mob:GetChildren()) do            if v.Name == _G.mode then             repeat task.wait() game:GetService("ReplicatedStorage").Document.Remote.Stats:FireServer("Melee","Up",1)             game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,-_G.re,0)  * CFrame.Angles(math.rad(90),0,0)             game:GetService("ReplicatedStorage").Document.Combat.Combat:FireServer()             until v.Humanoid.Health == 0 or not v:FindFirstChild("Humanoid") or not _G.autofarm                 end end end) end end else _G.autofarm = false end
end)
local Tab = Window:NewTab("tp Island")
local Section = Tab:NewSection("tp Island")
Section:NewButton("Castle island", "", function()
    local Local = game:GetService("TweenService")
local lll = TweenInfo.new( 20,Enum.EasingStyle.Linear,Enum.EasingDirection.Out,0,false,0 )
local papa = { ["CFrame"] = game:GetService("Workspace").Island["Castle island"]["Sand Island"].CFrame * CFrame.new(0,15,0)}
    local dwa = Local:Create(game.Players.LocalPlayer.Character.HumanoidRootPart,lll,papa)
    dwa:Play()
    local sa = Instance.new("BodyVelocity",game.Players.LocalPlayer.Character.HumanoidRootPart)
wait(20.2)
sa:Destroy()
end)
